    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "typescript": "~5.8.3"
  },
  "private": true,
  "engines": {
    "node": "20.17.0",
    "npm": ">=11.5.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
